{
  "name": "AlbumView",
  "version": "1.0.0",
  "description": "Album-oriented view of music collection data from Spotify and elsewhere",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "nf start",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client/ && npm install && npm run build",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run "
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "diacritic": "0.0.2",
    "express": "^4.17.1",
    "foreman": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.4",
    "moment": "^2.26.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "numbers2words": "^1.4.0",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.2.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "pusher": "^3.0.0",
    "simple-oauth2": "^3.3.0",
    "throng": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulphillips217/AlbumView.git"
  },
  "author": "Paul Phillips",
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulphillips217/AlbumView/issues"
  },
  "homepage": "https://github.com/paulphillips217/AlbumView#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "singleQuote": true
  }
}
